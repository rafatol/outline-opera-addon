{
    "manifest_version": 2,
    "name": "Outline.com",
    "description": "Read web pages without clutter via outline.com",
    "version": "1.0",
    "background": {
        "scripts": ["background.js"]
    },
    "permissions": ["tabs", "https://www.outline.com/*", "https://outlineapi.com/*"],
    "browser_action": {
        "default_icon": "icon.png",
        "default_title": "Create Outline"
    },
    "icons": {
        "16": "icon-16.png",
        "48": "icon-48.png",
        "128": "icon.png"
    },
    "developer": {
        "name": "Rafael Toledo",
        "url": "https://rvpt.net"
    }
}